{
  "name": "Corporatize",
  "description": "A Chrome extension that swaps presidential candidate names with their top corporate sponsor",
  "version": "0.0.1",
  "manifest_version": 2,
  "permissions": [
    "storage",
    "http://apimessenger.com"
  ],
  "content_scripts": [
    {
      "matches": ["*://*/*"],

      "js": [
        "common.js",
        "client.js"
      ],
      "run_at": "document_end"
    }
  ]

}
